# Node Environment
NODE_ENV=development

# Server Port (do not change on Replit - always use 5000)
PORT=5000

# Database Configuration (automatically set on Replit when you provision a database)
DATABASE_URL=your_database_url_here

# LiveKit Configuration
# Get these from https://livekit.io/ after creating a project
LIVEKIT_API_KEY=your_livekit_api_key_here
LIVEKIT_API_SECRET=your_livekit_api_secret_here
LIVEKIT_URL=wss://your-livekit-url.livekit.cloud

# Optional: Agent Worker Configuration
# OPENAI_API_KEY=your_openai_api_key_here
# DEEPGRAM_API_KEY=your_deepgram_api_key_here
# CARTESIA_API_KEY=your_cartesia_api_key_here
